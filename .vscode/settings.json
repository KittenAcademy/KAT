// Place your settings in this file to overwrite default and user settings.
{
	"editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/node_modules": true
    },
    "editor.insertSpaces": false,
    "editor.detectIndentation": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode"
}